#include <bits/stdc++.h>
using namespace std;

int main()
{
    int number_of_elements,number_to_find;
    cin >> number_of_elements >> number_to_find;
    int array_of_numbers[number_of_elements];
    for(int i=0;i<number_of_elements;i++)
    {
        cin >> array_of_numbers[i];
    }
    sort(array_of_numbers,array_of_numbers+number_of_elements);
    int left=0,right=number_of_elements-1;
    int answer=-1;
    while(left<=right)
    {
        int middle=left+(right-left)/2;
        if(number_to_find<array_of_numbers[middle])
        {
          right=middle-1;
        }
        else if(number_to_find>array_of_numbers[middle])
        {
            left=middle+1;
        }
        else
        {
            answer=middle;
            break;
        }
    }
    if(answer!=-1)
    {
    cout << answer+1;
    } 
    else
    {
    cout << -1;
    }

       return 0;
}
